## Default Kit for Players Without a Kit ID
# armor
item replace entity @s armor.head with iron_helmet[unbreakable={}]
item replace entity @s armor.chest with iron_chestplate[unbreakable={}]
item replace entity @s armor.legs with netherite_leggings[unbreakable={}]
item replace entity @s armor.feet with netherite_boots[unbreakable={}]

# weapons
give @s iron_sword[attribute_modifiers={modifiers:[{type:attack_damage,amount:5,operation:add_value,id:1741893913625},{type:attack_speed,amount:20,operation:add_value,id:1741893913625}]},unbreakable={}]
give @s netherite_axe[attribute_modifiers={modifiers:[{type:attack_damage,amount:10,operation:add_value,id:1741893957040},{type:attack_speed,amount:-3,operation:add_value,id:1741893957040}]},unbreakable={}]